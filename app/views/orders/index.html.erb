<!DOCTYPE html>
<html>
<head>
	<title>Dashboard</title>
	</head>

<body>
	<div class="container">
		<div>
			<%= flash[:notice] %><br>
			<b>Total Data Witel Kalteng :</b> <%= Order.count %><br>
			<b>Today :</b> <%= Date.today %><br>
			<b>PSB : </b><%= @psb.css("table tr[bgcolor='']").count %>
		</div>
		<hr>
		<br>
		<%= render(:partial => "dashboard1") %>
		<%= render(:partial => "dashboard2") %>
		<br><br>	
		<hr>		
		<div class="row">
			<div class="col-md-6">
				<h4>Import data</h4>

				<%= link_to "Update Data", starclick_url %>

				<h4>Export Data</h4>
				Download:
				<%= link_to "CSV", orders_path(format: "csv") %> |
				<%= link_to "Excel", orders_path(format: "xls") %>
			</div>
		</div>	
	</div>
</body>
</html>

